        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cqu.demo.dao.UserBaseInfoMapper">
    <select id="getUserBaseInfo" parameterType="string" resultType="cn.cqu.demo.model.UserBaseInfo">
